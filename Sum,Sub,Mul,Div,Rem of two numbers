#include<stdio.h>
#include<math.h>
 int main()
 {
     int a=60 , b=30 , sum, sub,mul,div,rem ;
  sum=a+b;
    printf("sum= %d\n", sum);
  sub=a-b;
    printf("sub= %d\n",sub);
  mul=a*b;
    printf("mul= %d\n",mul);
  div=a/b;
    printf("div= %d\n",div);
  rem=a%b;
    printf("rem= %d\n",rem);
 return 0;

 }
