include<stdio.h>
int main()
{
   int a;
   float b;
   double c;
   char d;

    printf("the size of integer = %d bytes\n",sizeof(a));
    printf("the size of float = %d bytes\n",sizeof(b));
    printf("the size of double = %d bytes\n",sizeof(c));
    printf("the size of character = %d bytes\n",sizeof(d));

   return 0;
}
